@require "~cube-ui/src/common/stylus/var/color.styl" $dialog-bgc :=#fff;
.cube-dialog-btns {
    display: none;
}

.cube-dialog-content {
    margin: 0;
}

.my-title {
    width: 100%;
    font-size: 0.48rem;
    margin: 0.67rem auto;
}

.my-prize-content {
    width: 100%;
    padding: 0 0.4rem;
    box-sizing: border-box;
    overflow: auto;
    .my-content-table {
        width: 100%;
        margin-bottom: 1.2rem;
        tr>th {
            text-align: center;
            height: 1.067rem;
            line-height: 1.067rem;
            background: #F0F0F0;
        }
        tr>td {
            text-align: center;
            height: 1.067rem;
            line-height: 1.067rem;
            border-top: 1px solid #E4E4E4;
            border-bottom: 1px solid #E4E4E4;
            white-space: nowrap;
            overflow: hidden;
            text-overflow: ellipsis;
        }
    }
}

.my-content {
    width: 100%;
    padding: 0 0.4rem;
    box-sizing: border-box;
    overflow: auto;
    .my-content-table {
        width: 100%;
        color: #363636;
        tr>th {
            text-align: center;
            height: 1.067rem;
            line-height: 1.067rem;
            background: #F0F0F0;
        }
        th:first-child {
            width: 1.53rem;
        }
        th:nth-child(2) {
            width: 2.56rem;
        }
        th:nth-child(3) {
            width: 2.56rem;
        }
        tr>td {
            text-align: center;
            height: 1.067rem;
            line-height: 1.067rem;
            border-top: 1px solid #E4E4E4;
            border-bottom: 1px solid #E4E4E4;
        }
    }
    .my-content-tip {
        width: 100%;
        font-size: 0.32rem;
        text-align: center;
        margin: 0.28rem auto;
        color: #FC4C5A;
    }
}

.my-period-integral {
    height: 13.0266rem;
    padding: 0 0.4rem;
    box-sizing: border-box;
    overflow: auto;
    line-height: 1.067rem;
}

.my-title-span {
    width: 100%;
    height: 0.2rem;
    margin: 0.16rem auto;
    text-align: center;
    color: #999;
    font-size: 0.32rem;
}

.my-prize {
    width: 100%;
    background: #f0f0f0;
    height: 0.8333rem;
    display: inline-block;
    font-size: 0.373333rem;
    box-sizing: border-box;
    margin-top: 0.6666rem;
    position: absolute;
    bottom: 0;
    right: 0;
}

.my-prize button {
    width: 50%;
    height: 0.52rem;
    background: #f0f0f0;
    display: inline-block;
    font-size: 0.373333rem;
    text-align: center;
    border: none;
    border-right: 2px solid #ccc;
    margin-top: 0.16rem;
}

.my-prize button:nth-child(2) {
    border-right: none;
    color: #FC4C5A;
    height: 0.53333rem;
}

.my-title-rule {
    width: 100%;
    height: 0.63rem;
    margin: 0 auto;
    line-height: 0.63rem;
    font-size: 0.48rem;
    font-family: MicrosoftYaHei;
    color: rgba(54, 54, 54, 1);
    text-align: center;
}

.dialog-rule {
    width: 100%;
    margin-bottom: 0.2rem;
    padding: 0.1rem 0;
    h5 {
        width: 100%;
        height: 0.67rem;
        line-height: 0.67rem;
        font-size: 0.37rem;
        font-family: PingFangSC-Medium;
        color: rgba(54, 54, 54, 1);
        font-weight: 600;
    }
    div {
        color: '#363636';
        width: 100%;
        line-height: 0.53rem;
    }
}

.cube-input {
    color: #363636;
}

.cube-input-field {
    color: #757575;
    height: 0.8rem;
    padding: 0;
}

.register-wrap .cube-input::after {
    border: none
}

.cube-btn {
    background: rgba(64, 129, 214, 1);
    height: 40px;
    line-height: 0;
}

.my-winning-title {
    margin-top: 0.6410rem;
    text-align: center;
    color: rgba(54, 54, 54, 1);
    font-family: PingFangSC-Regular;
    font-size: 0.48rem;
}

.my-content-winning-title {
    color: rgba(54, 54, 54, 1);
    font-family: PingFangSC-Regular;
    font-size: 0.48rem;
}

.my-content-winning-prize {
    margin: 0 auto;
    text-align: center;
    font-family: PingFangSC-Semibold;
    color: rgba(252, 76, 90, 1);
}

.my-content-winning-img {
    margin: 0 auto;
    width: 5.6667rem;
    height: 4.2533rem;
    background: url("~images/dow-ceremony/trophy.png") no-repeat;
    background-size: 100%;
}

.my-content-winning-button {
    margin: 0.32rem auto;
    width: 4.8rem;
    height: 1.2rem;
    line-height: 1.2rem;
    background: linear-gradient(132.9deg, rgba(255, 152, 91, 1), rgba(255, 63, 79, 1));
    border-radius: 0.6rem;
    font-size: 0.4266rem;
    text-align: center;
    color: rgba(255, 255, 255, 1);
    font-family: PingFangSC-Semibold;
}

.my-content-download-title {
    color: rgba(54, 54, 54, 1);
    font-family: PingFangSC-Regular;
    font-size: 0.48rem;
}

.my-content-download-img {
    margin: 0.73rem auto 0;
    width: 5.6667rem;
    height: 4.2533rem;
    background: url("~images/dow-ceremony/downloadApp.png") no-repeat;
    background-size: 100%;
}

.my-content-download-app {
    margin: 0.36rem auto 0;
    text-align: center;
    font-family: PingFangSC-Semibold;
    color: #363636;
}

.my-content-download-points {
    margin: 0 auto;
    text-align: center;
    font-family: PingFangSC-Semibold;
    color: #363636;
}

.my-content-download-button {
    margin: 0.53rem auto;
    width: 4.8rem;
    height: 1.2rem;
    line-height: 1.2rem;
    background: linear-gradient(132.9deg, rgba(255, 152, 91, 1), rgba(255, 63, 79, 1));
    border-radius: 0.6rem;
    font-size: 0.4266rem;
    text-align: center;
    color: rgba(255, 255, 255, 1);
    font-family: PingFangSC-Semibold;
}

.login-input.cube-input::after {
    border-bottom: 1px solid #E4E4E4;
    border-right: 1px solid #fff;
    border-left: 1px solid #fff;
    border-top: 1px solid #fff;
}

.detail-input.cube-input::after {
    border: none;
}

.wheel-scroll {
    li[class*='active'] {
        color: #fc4c5a;
    }
}

.season-picker>[data-action="confirm"] {
    color: #fc4c5a;
}

.notice-prize #activPrize li {
    z-index: 9 !important;
}

.cube-dialog-main {
    width: 8rem;
}

.area-picker>[data-action="confirm"] {
    color: #4081D6;
}

.my-prize-content-dialog {
    width: 100%;
    .my-content-prize-table {
        margin: 0 auto;
        width: 90%;
        padding: 0.27rem 0;
        border-top: 1px solid #E4E4E4;
        border-bottom: 1px solid #E4E4E4;
        color: #363636;
        tr,
        th,
        td {
            border: none;
            padding: 0.2rem 0;
            text-align left;
            0
        }
        td:first-child {
            width: 2.24rem;
            white-space: nowrap;
        }
        td:nth-child(2) {
            padding-left: 0.4rem;
            box-sizing border-box;
        }
    }
}

.my-prize-button {
    margin-top: 0.67rem;
    width: 100%;
    height: 1.2rem;
    background: #F0F0F0;
    line-height: 1.2rem;
    button {
        cursor pointer;
        width: 50%;
        height: 0.53rem;
        color: #999999;
        border: none;
        outline none;
        background: #F0F0F0;
        font-size: 0.373rem;
    }
    button:first-child {
        border-right: 1px solid #CCCCCC;
    }
}

.cube-picker-choose {
    height: 1.067rem;
    line-height: 1.067rem;
    span {
        position: absolute !important;
        top: -0.45rem !important;
        height: 1.067rem !important;
        width: 1rem !important;
        margin: 0 !important;
    }
}

.cube-picker-choose>[data-action="cancel"] {
    color: #757575;
}

.cube-picker-choose>[data-action="confirm"] {
    color: #FC4C5A;
}

.area-picker>[data-action="confirm"] {
    color: #4081d6;
}

.area-picker>[data-action="confirm"]:active {
    color: #4081d6;
}

.cube-picker-choose>h1 {
    height: 1.067rem;
    line-height: 1.067rem;
    font-size: 0.373rem;
    color: #363636;
}

.selected-district {
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
    width: 5rem;
}

.cube-picker-confirm {
    color: #4081D6;
    right: 0;
}

.close-resigter-content {
    text-align: center;
    margin-top: 0.8rem;
    p {
        color: #4E4E4E;
        font-size: 0.373rem;
        line-height: 0.48rem;
    }
    .known-btn {
        height: 1.2rem;
        line-height: 1.2rem;
        display: block;
        font-size: 0.373rem;
        color: #4081D6;
        background: #f0f0f0;
        margin-top: 0.8rem;
    }
}

.input-mileage-content {
    p {
        width: 100%;
        margin-top: 0.4rem;
        text-align: center;
        height: 0.666667rem;
        font-size: 0.48rem;
        color: #363636;
        line-height: 0.666667rem;
    }
    .mileage-input {
        height: 0.88rem;
        margin: 0.2rem 1.333333rem 0.405405rem;
        padding: 0.04rem 0.266667rem;
        font-size: 0.373333rem;
        line-height: 0.533333rem;
        border-radius: 0.08rem 0.08rem;

        &.cube-input_active::after {
            border-color: #999999;
        }
    }

    .btn-groups {
        text-align: center;
        margin-bottom: 0.4rem;

        >span {
            display: inline-block;
            width: 1.693333rem;
            height: 0.72rem;
            border-radius: 0.08rem;
            text-align: center;
            font-size: 0.44rem;
            line-height: 0.72rem;
        }
        .cancel-btn {
            background: #e4e4e4;
            color: #ffffff;
            margin-right: 1.413333rem;
        }
        .submit-btn {
            background: #ff9d4d;
            color: #ffffff;
        }
    }
}